/target/
/bin/
/out/
/build/
/classes/
/lib/
/dist/

# Maven
.mvn/
mvnw
mvnw.cmd
.pom.xml.tag
.pom.xml.releaseBackup
.pom.xml.versionsBackup
.pom.xml.next
release.properties
dependency-reduced-pom.xml
buildNumber.properties
