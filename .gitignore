.idea/
*.iml
*.ipr
*.iws
*.ids
*.orig
out/
local.properties

# Eclipse
.project
.classpath
.settings/
bin/
.metadata/

# NetBeans
nbproject/private/
build/
nbbuild/
dist/
nbdist/
.nb-gradle/

# VS Code (可选，根据团队习惯)
.vscode/
!.vscode/settings.json
!.vscode/tasks.json
!.vscode/launch.json
!.vscode/extensions.json

